<script src="/js/scripts.js" charset="utf-8"></script>


<!-- Templates -->
<script id="allstories-template" type="text/x-handlebars-template">
  {{each stories}}
    <div>
      <h4> {{ title }} </h4>
      <h5> {{ date }} </h5>
      <h5> {{ tag1 }} </h5>
      <h5> {{ tag2 }} </h5>
      <h5> {{ tag3 }} </h5>
      <h5> {{ story }} </h5>
    </div>
</script>
